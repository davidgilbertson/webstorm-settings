<templateSet group="My Templates">
  <template name="cl" value="console.log('  --  &gt;  $message$');" description="console log" toReformat="false" toShortenFQNames="true">
    <variable name="message" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="cv" value="console.log('  --  &gt;  $var$:', $var$);" description="console log a variable" toReformat="false" toShortenFQNames="true">
    <variable name="var" expression="clipboard()" defaultValue="var" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ci" value="console.log('  --  &gt;  $file$:$line$ &gt; $func$()'$END$);" description="console log some info about this method" toReformat="false" toShortenFQNames="true">
    <variable name="file" expression="fileName()" defaultValue="file" alwaysStopAt="false" />
    <variable name="line" expression="lineNumber()" defaultValue="line" alwaysStopAt="false" />
    <variable name="func" expression="jsMethodName()" defaultValue="function" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="todo" value="// TODO (davidg): " description="// TODO(davidg)" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="true" />
      <option name="XSL_TEXT" value="true" />
      <option name="XML" value="true" />
      <option name="XML_TEXT" value="true" />
      <option name="JSON" value="true" />
      <option name="JSP" value="true" />
      <option name="CSS_PROPERTY_VALUE" value="true" />
      <option name="CSS_DECLARATION_BLOCK" value="true" />
      <option name="CSS_RULESET_LIST" value="true" />
      <option name="CSS" value="true" />
      <option name="CUCUMBER_FEATURE_FILE" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="TypeScript" value="true" />
      <option name="Handlebars" value="true" />
      <option name="HAML" value="true" />
      <option name="JADE" value="true" />
      <option name="CoffeeScript" value="true" />
      <option name="DART" value="true" />
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="mql" value="@media #{$large-up} {&#10;    $END$&#10;}" description="Media query large-up" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_PROPERTY_VALUE" value="true" />
      <option name="CSS_DECLARATION_BLOCK" value="true" />
      <option name="CSS_RULESET_LIST" value="true" />
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="mqs" value="@media #{$small-only} {&#10;    $END$&#10;}" description="Media query small-up" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_PROPERTY_VALUE" value="true" />
      <option name="CSS_DECLARATION_BLOCK" value="true" />
      <option name="CSS_RULESET_LIST" value="true" />
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="mqm" value="@media #{$medium-up} {&#10;    $END$&#10;}" description="Media query medium-up" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_PROPERTY_VALUE" value="true" />
      <option name="CSS_DECLARATION_BLOCK" value="true" />
      <option name="CSS_RULESET_LIST" value="true" />
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="mqxl" value="@media #{$xlarge-up} {&#10;    $END$&#10;}" description="Media query extra-large-up" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_PROPERTY_VALUE" value="true" />
      <option name="CSS_DECLARATION_BLOCK" value="true" />
      <option name="CSS_RULESET_LIST" value="true" />
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="civ" value="console.log('  --  &gt;  $file$:$line$ &gt; $func$() &gt; $var$:', $var$);" description="console log some info about this method" toReformat="false" toShortenFQNames="true">
    <variable name="file" expression="fileName()" defaultValue="file" alwaysStopAt="false" />
    <variable name="line" expression="lineNumber()" defaultValue="line" alwaysStopAt="false" />
    <variable name="func" expression="jsMethodName()" defaultValue="function" alwaysStopAt="false" />
    <variable name="var" expression="clipboard()" defaultValue="var" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="imp" value="import $var1$ from '$var2$';" description="import x from 'x'" toReformat="false" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="var2" expression="" defaultValue="$var1$" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="pj" value="&lt;pre&gt;{JSON.stringify(data, null, 2)}&lt;/pre&gt;" description="&lt;pre&gt;{JSON.stringify(data, null, 2)}&lt;/pre&gt;" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="cid" value="console.log('  --  &gt;  $file$:$line$ &gt; $func$() $var$:');&#10;console.dir($var$, {depth: $END$1, colors: true});" description="console log  and dir some info about this method" toReformat="false" toShortenFQNames="true">
    <variable name="file" expression="fileName()" defaultValue="" alwaysStopAt="false" />
    <variable name="line" expression="lineNumber()" defaultValue="" alwaysStopAt="false" />
    <variable name="func" expression="jsMethodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="var" expression="clipboard()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>